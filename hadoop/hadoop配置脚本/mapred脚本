mapred脚本

<configuration>
    <!--指定Mapreduce程序运行在yarn上-->
		<property>
			<name>mapreduce.framework.name</name>
			<value>yarn</value>
		</property>
	<!--历史服务器端地址-->
		<property>
			<name>mapreduce.jobhistory.address</name>
			<value>hadoop102:10020</value>
		</property>

	<!--历史服务器web端地址-->
		<property>
			<name>mapreduce.jobhistory.webapp.address</name>
			<value>hadoop102:19888</value>
		</property>
		
</configuration>